{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "gdb",
            "request": "launch",
            "name": "Launch Program (SSH)",
            "target": "${fileBasenameNoExtension}",
            "cwd": "${fileDirname}",
            "ssh": {
                "host": "127.0.0.1",
                "port": 2222,
                "keyfile": "/root/.ssh/debug",
                "cwd": "",
                "user": "root",
                "forwardX11": false
            },
            "gdbpath": "gdb",
            "printCalls": true,
            "autorun": [
                "set mi-async",
                "set debuginfod enabled on"
            ]
        }
    ],
    "inputs": []
}
